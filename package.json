        "url": "git+https://github.com/ArtiomTr/ncfg.git"
    },
    "bugs": {
        "url": "https://github.com/ArtiomTr/ncfg/issues"
    },
    "devDependencies": {
        "@size-limit/preset-small-lib": "^5.0.5",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.10.1",
        "aqu": "0.2.2",
        "husky": "^7.0.2",
        "lint-staged": "^11.1.2",
        "np": "^7.5.0",
        "size-limit": "^5.0.5",
        "typescript": "^4.4.3"
    },
    "types": "./dist/ncfg.d.ts"
