    "name": "ncfg",
    "version": "0.0.0",
    "description": "",
    "main": "./dist/index.js",
    "module": "./dist/ncfg.esm.js",
    "files": [
        "dist"
    ],
    "size-limit": [
        {
            "path": "dist/ncfg.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/ncfg.esm.js",
            "limit": "10 KB"
        }
    ],
    "scripts": {
        "build": "aqu build",
        "start": "aqu watch",
        "lint": "eslint .",
        "lint:fix": "aqu lint --fix",
        "test": "aqu test --passWithNoTests",
        "test:watch": "aqu test --passWithNoTests --watch",
        "test:coverage": "aqu test --ci --json --coverage --testLocationInResults --outputFile=\"report.json\"",
        "prepare": "aqu build",
        "release": "np",
        "size": "size-limit",
        "size:why": "size-limit --why"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx,json}": "npm run lint:fix"
